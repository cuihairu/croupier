{
    "version":  "2.0.0",
    "tasks":  [
                  {
                      "label":  "proto:install-doc-plugin",
                      "problemMatcher":  [

                                         ],
                      "command":  "go",
                      "type":  "shell",
                      "args":  [
                                   "install",
                                   "github.com/pseudomuto/protoc-gen-doc/cmd/protoc-gen-doc@latest"
                               ],
                      "options":  {
                                      "cwd":  ""
                                  }
                  },
                  {
                      "label":  "proto:gen",
                      "problemMatcher":  [

                                         ],
                      "command":  "buf",
                      "type":  "shell",
                      "args":  [
                                   "generate"
                               ],
                      "options":  {
                                      "cwd":  ""
                                  }
                  },
                  {
                      "label":  "proto:docs (md+html)",
                      "problemMatcher":  [

                                         ],
                      "command":  "buf",
                      "type":  "shell",
                      "args":  [
                                   "generate",
                                   "--template",
                                   "buf.gen.docs.yaml"
                               ],
                      "options":  {
                                      "cwd":  ""
                                  }
                  },
                  {
                      "label":  "proto:docs",
                      "group":  {
                                    "kind":  "build",
                                    "isDefault":  true
                                },
                      "dependsOn":  [
                                        "proto:install-doc-plugin",
                                        "proto:docs (md+html)"
                                    ],
                      "dependsOrder":  "sequence"
                  }
              ]
}
