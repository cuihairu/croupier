#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*|*MINGW*|*MSYS*)
        if command -v cygpath > /dev/null 2>&1; then
            basedir=`cygpath -w "$basedir"`
        fi
    ;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/cui/Workspaces/croupier/docs_new/node_modules/.pnpm/rtlcss@3.5.0/node_modules/rtlcss/bin/node_modules:/Users/cui/Workspaces/croupier/docs_new/node_modules/.pnpm/rtlcss@3.5.0/node_modules/rtlcss/node_modules:/Users/cui/Workspaces/croupier/docs_new/node_modules/.pnpm/rtlcss@3.5.0/node_modules:/Users/cui/Workspaces/croupier/docs_new/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/Users/cui/Workspaces/croupier/docs_new/node_modules/.pnpm/rtlcss@3.5.0/node_modules/rtlcss/bin/node_modules:/Users/cui/Workspaces/croupier/docs_new/node_modules/.pnpm/rtlcss@3.5.0/node_modules/rtlcss/node_modules:/Users/cui/Workspaces/croupier/docs_new/node_modules/.pnpm/rtlcss@3.5.0/node_modules:/Users/cui/Workspaces/croupier/docs_new/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../bin/rtlcss.js" "$@"
else
  exec node  "$basedir/../../bin/rtlcss.js" "$@"
fi
