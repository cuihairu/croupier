#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*|*MINGW*|*MSYS*)
        if command -v cygpath > /dev/null 2>&1; then
            basedir=`cygpath -w "$basedir"`
        fi
    ;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/cui/Workspaces/croupier/docs_new/node_modules/.pnpm/@docusaurus+core@2.4.3_@docusaurus+types@2.4.3_react-dom@17.0.2_react@17.0.2__react@17._f812443be7e4695718f5e0935340ca14/node_modules/@docusaurus/core/bin/node_modules:/Users/cui/Workspaces/croupier/docs_new/node_modules/.pnpm/@docusaurus+core@2.4.3_@docusaurus+types@2.4.3_react-dom@17.0.2_react@17.0.2__react@17._f812443be7e4695718f5e0935340ca14/node_modules/@docusaurus/core/node_modules:/Users/cui/Workspaces/croupier/docs_new/node_modules/.pnpm/@docusaurus+core@2.4.3_@docusaurus+types@2.4.3_react-dom@17.0.2_react@17.0.2__react@17._f812443be7e4695718f5e0935340ca14/node_modules/@docusaurus/node_modules:/Users/cui/Workspaces/croupier/docs_new/node_modules/.pnpm/@docusaurus+core@2.4.3_@docusaurus+types@2.4.3_react-dom@17.0.2_react@17.0.2__react@17._f812443be7e4695718f5e0935340ca14/node_modules:/Users/cui/Workspaces/croupier/docs_new/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/Users/cui/Workspaces/croupier/docs_new/node_modules/.pnpm/@docusaurus+core@2.4.3_@docusaurus+types@2.4.3_react-dom@17.0.2_react@17.0.2__react@17._f812443be7e4695718f5e0935340ca14/node_modules/@docusaurus/core/bin/node_modules:/Users/cui/Workspaces/croupier/docs_new/node_modules/.pnpm/@docusaurus+core@2.4.3_@docusaurus+types@2.4.3_react-dom@17.0.2_react@17.0.2__react@17._f812443be7e4695718f5e0935340ca14/node_modules/@docusaurus/core/node_modules:/Users/cui/Workspaces/croupier/docs_new/node_modules/.pnpm/@docusaurus+core@2.4.3_@docusaurus+types@2.4.3_react-dom@17.0.2_react@17.0.2__react@17._f812443be7e4695718f5e0935340ca14/node_modules/@docusaurus/node_modules:/Users/cui/Workspaces/croupier/docs_new/node_modules/.pnpm/@docusaurus+core@2.4.3_@docusaurus+types@2.4.3_react-dom@17.0.2_react@17.0.2__react@17._f812443be7e4695718f5e0935340ca14/node_modules:/Users/cui/Workspaces/croupier/docs_new/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../bin/docusaurus.mjs" "$@"
else
  exec node  "$basedir/../../bin/docusaurus.mjs" "$@"
fi
