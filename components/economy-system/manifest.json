    },
    {
      "id": "economy.trade.create",
      "version": "1.0.0",
      "enabled": true,
      "description": "Create trade offer between players"
    },
    {
      "id": "economy.marketplace.list",
      "version": "1.0.0",
      "enabled": true,
      "description": "List items on marketplace"
    }
