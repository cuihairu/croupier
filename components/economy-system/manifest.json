{
  "id": "economy-system",
  "name": "Virtual Economy System",
  "version": "1.0.0",
  "description": "Virtual currency, item trading, and marketplace functionality",
  "category": "economy",
  "dependencies": ["player-management"],
  "functions": [
    {
      "id": "economy.currency.add",
      "version": "1.0.0",
      "enabled": true,
      "description": "Add virtual currency to player account"
    },
    {
      "id": "economy.currency.deduct",
      "version": "1.0.0",
      "enabled": true,
      "description": "Deduct virtual currency from player account"
    },
    {
      "id": "economy.item.grant",
      "version": "1.0.0",
